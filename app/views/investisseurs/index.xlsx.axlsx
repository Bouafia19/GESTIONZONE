wb = xlsx_package.workbook
style_shout = wb.styles.add_style sz: 16, b: true, alignment: { horizontal: :center }
wb.add_worksheet(name: "Foobar") do |sheet|
   @investisseurs.each do |s|
sheet.add_row [s.raison_sociale_francais,s.num_dossier,s.date_depot_demande,s.raison_sociale_arabe,s.nom_prenom_gerant_arabe,s.raison_sociale_francais,s.nom_prenom_gerant_francais,s.adresse,s.num_telephone,s.fax]
end
end
