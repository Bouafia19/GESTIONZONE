<p id="notice"><%= notice %></p>

<h1>Extensions</h1>

<table>
  <thead>
    <tr>
      <th>N° de lot</th>
      <th>N dossier</th>
      <th>R socia ar</th>
      <th>R socia fr</th>
      <th>Geom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extensions.each do |extension| %>
      <tr>
        <td><%= extension.n°_de_lot %></td>
        <td><%= extension.n_dossier %></td>
        <td><%= extension.r_socia_ar %></td>
        <td><%= extension.r_socia_fr %></td>
        <td><%= extension.geom %></td>
        <td><%= link_to 'Show', extension %></td>
        <td><%= link_to 'Edit', edit_extension_path(extension) %></td>
        <td><%= link_to 'Destroy', extension, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extension', new_extension_path %>
